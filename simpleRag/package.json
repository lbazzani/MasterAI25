    "@qdrant/js-client-rest": "^1.11.0",
    "openai": "^4.73.0",
    "pdf-parse": "^1.1.1",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/pdf-parse": "^1.1.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
